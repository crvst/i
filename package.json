    "react-addons-shallow-compare": "^15.4.2",
    "react-css-modules": "^4.1.0",
    "react-dom": "^15.4.1",
    "react-helmet": "^3.2.2",
    "react-measure": "^1.4.5",
    "react-redux": "^4.4.6",
    "react-responsive": "^1.2.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-select": "^1.0.0-rc.2",
    "redux": "^3.6.0",
