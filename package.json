    "sanitize-html": "^1.13.0",
    "sanitize.css": "^4.1.0",
    "tachyons": "^4.5.5"
  },
  "scripts": {
    "--prepush": "npm t && npm run stylint && npm run eslint",
    "build": "npm run svg -s && node scripts/build.js",
    "eslint": "eslint --ext .jsx,.js  src/**/ --fix",
    "start": "npm run svg -s && webpack-dashboard -- node scripts/start.js",
    "stylint": "stylint",
    "svg": "svg-sprite --symbol --symbol-inline --symbol-example --shape-id-generator=icon- --symbol-sprite=icons.svg --dest=src/assets src/assets/icons/svg/**/*.svg",
    "test": "node scripts/test.js --env=jsdom",
    "purifycss": "purifycss build/static/css/app.css build/static/js/app.bundle.js --min --rejected --info --min --rejected"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.(css|styl)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "jasmine-enzyme"
    ]
  },
  "plugins": [
    "transform-flow-strip-types"
  ],
  "eslintConfig": {
    "extends": [
      "airbnb",
      "plugin:flowtype/recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:jsx-control-statements/recommended"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "babel",
      "compat",
      "eslint-plugin-flowtype",
      "flowtype",
      "flowtype-errors",
      "immutable",
      "json",
      "jsx-control-statements",
      "no-inferred-method-name",
      "no-use-extend-native",
      "promise",
      "react",
      "unicorn"
    ],
    "rules": {
      "arrow-parens": [
        2,
        "always"
      ],
      "compat/compat": 1,
      "flowtype-errors/show-errors": 2,
      "immutable/no-let": 2,
      "immutable/no-mutation": 1,
      "immutable/no-this": 0,
      "import/prefer-default-export": 1,
      "jsx-a11y/no-static-element-interactions": 1,
      "jsx-control-statements/jsx-use-if-tag": 2,
      "max-len": 1,
      "no-confusing-arrow": 1,
      "no-return-assign": 1,
      "no-unused-vars": 1,
      "promise/always-return": 2,
      "promise/avoid-new": 1,
      "promise/catch-or-return": 2,
      "promise/no-callback-in-promise": 1,
      "promise/no-native": 0,
      "promise/no-nesting": 1,
      "promise/no-promise-in-callback": 1,
      "promise/no-return-wrap": 2,
      "promise/param-names": 2,
      "react/forbid-prop-types": 1,
      "react/jsx-first-prop-new-line": 2,
      "react/jsx-handler-names": 2,
      "react/jsx-no-bind": 1,
      "react/no-children-prop": 0,
      "react/require-optimization": 2,
      "unicorn/catch-error-name": [
        2,
        {
          "name": "err"
        }
      ],
      "unicorn/no-abusive-eslint-disable": 2,
      "unicorn/no-process-exit": 2,
      "unicorn/throw-new-error": 2,
      "no-constant-condition": [
        1,
        {
          "checkLoops": true
        }
      ]
    }
  }
}
